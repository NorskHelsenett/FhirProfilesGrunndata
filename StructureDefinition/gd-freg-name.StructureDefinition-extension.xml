<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="gd-freg-name" />
  <url value="http://ehelse.no/fhir/StructureDefinition/gd-freg-name" />
  <version value="0.1" />
  <name value="gd-freg-name" />
  <status value="draft" />
  <date value="2019-04-11" />
  <description value="metadata about human name from FREG (folkeregisterpersonnavn) describes metadata about HumanName available from the norwegian person index (folkeregisteret, freg)" />
  <fhirVersion value="3.0.1" />
  <kind value="complex-type" />
  <abstract value="false" />
  <contextType value="datatype" />
  <context value="HumanName" />
  <type value="Extension" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Extension" />
  <derivation value="constraint" />
  <differential>
    <element id="Extension">
      <path value="Extension" />
      <short value="metadata about human name" />
      <definition value="metadata about human name from FREG (folkeregisterpersonnavn) describes metadata about HumanName available from the norwegian person index (folkeregisteret, freg)" />
      <alias value="folkeregisterpersonnavn" />
      <base>
        <path value="Element" />
        <min value="0" />
        <max value="*" />
      </base>
      <constraint>
        <key value="ext-1" />
        <severity value="error" />
        <human value="Must have either extensions or value[x], not both" />
        <expression value="extension.exists() != value.exists()" />
        <xpath value="exists(f:extension)!=exists(f:*[starts-with(local-name(.), 'value')])" />
      </constraint>
      <mapping>
        <identity value="rim" />
        <map value="N/A" />
      </mapping>
    </element>
    <element id="Extension.extension">
      <path value="Extension.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Extension.extension:shortName">
      <path value="Extension.extension" />
      <sliceName value="shortName" />
      <short value="forkortetNavn fra FREG" />
      <definition value="personnavn som benyttes ved maskinelle utskrifter" />
      <alias value="forkortetNavn" />
      <max value="1" />
    </element>
    <element id="Extension.extension:shortName.url">
      <path value="Extension.extension.url" />
      <fixedUri value="shortName" />
    </element>
    <element id="Extension.extension:shortName.value[x]:valueString">
      <path value="Extension.extension.valueString" />
      <sliceName value="valueString" />
      <type>
        <code value="string" />
      </type>
    </element>
    <element id="Extension.extension:originalName">
      <path value="Extension.extension" />
      <sliceName value="originalName" />
      <short value="originaltNavn fra FREG" />
      <definition value="personens navn iht. godkjent identifikasjonsdokument ved opprettelse av en folkeregisterperson i Folkeregisteret&#xD;&#xA;&#xD;&#xA;Dette er et nytt felt i Folkeregisteret og vil bli benyttet tidligst etter at Registeret er master.&#xD;&#xA;&#xD;&#xA;Når Registeret er master så kan det bli benyttet som følger:&#xD;&#xA;Dette navnet vil bli skrevet på originalt tegnsett fra identifikasjonsdokumentet. Dette vil bli støttet av tegnsettet ISO 10646 lagret på UTF-8 format og vi kan derfor i praksis lagre alle verdens tegn.&#xD;&#xA;Denne datatypen inneholder fornavn, mellomnavn og etternavn, men reglene for innhold etter Navneloven gjelder ikke for utenlandske navn på original form.&#xD;&#xA;" />
      <alias value="originaltNavn" />
      <max value="1" />
    </element>
    <element id="Extension.extension:originalName.url">
      <path value="Extension.extension.url" />
      <fixedUri value="originalName" />
    </element>
    <element id="Extension.extension:originalName.value[x]:valueHumanName">
      <path value="Extension.extension.valueHumanName" />
      <sliceName value="valueHumanName" />
      <type>
        <code value="HumanName" />
      </type>
    </element>
    <element id="Extension.url">
      <path value="Extension.url" />
      <fixedUri value="http://ehelse.no/fhir/StructureDefinition/gd-freg-name" />
    </element>
    <element id="Extension.value[x]">
      <path value="Extension.value[x]" />
      <max value="0" />
    </element>
  </differential>
</StructureDefinition>