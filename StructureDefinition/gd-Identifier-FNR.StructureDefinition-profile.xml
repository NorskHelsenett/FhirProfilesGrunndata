<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="gd-Identifier-FNR" />
  <url value="http://ehelse.no/fhir/StructureDefinition/gd-Identifier-FNR" />
  <version value="0.1" />
  <name value="gd-Identifier-FNR" />
  <status value="draft" />
  <date value="2019-03-05" />
  <description value="Identifier profile defining Norwegian fødselsnummer identifier from Folkeregisteret" />
  <fhirVersion value="3.0.1" />
  <kind value="complex-type" />
  <abstract value="false" />
  <type value="Identifier" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Identifier" />
  <derivation value="constraint" />
  <differential>
    <element id="Identifier">
      <path value="Identifier" />
      <short value="Norwegian fødselsnummer" />
      <definition value="Fødselsnummer for norwegian citizen. Fødselsnummer as one of the possible patient/person identifier." />
      <alias value="FNR" />
      <alias value="fødselsnummer" />
      <base>
        <path value="Element" />
        <min value="0" />
        <max value="*" />
      </base>
      <mapping>
        <identity value="v2" />
        <map value="CX / EI (occasionally, more often EI maps to a resource id or a URL)" />
      </mapping>
      <mapping>
        <identity value="rim" />
        <map value="II - see see identifier pattern at http://wiki.hl7.org/index.php?title=Common_Design_Patterns#Identifier_Pattern for relevant discussion. The Identifier class is a little looser than the v3 type II because it allows URIs as well as registered OIDs or GUIDs.  Also maps to Role[classCode=IDENT]" />
      </mapping>
      <mapping>
        <identity value="servd" />
        <map value="Identifier" />
      </mapping>
    </element>
    <element id="Identifier.system">
      <path value="Identifier.system" />
      <short value="Identification of the Norwegian FNR" />
      <definition value="Identifies that this is a Norwegian Fødselsnummer" />
      <min value="1" />
      <fixedUri value="urn:oid:2.16.578.1.12.4.1.4.1" />
    </element>
    <element id="Identifier.value">
      <path value="Identifier.value" />
      <short value="The actual Fødselsnummer" />
      <definition value="The Norwegian Fødselsnummer" />
      <alias value="fødselsnummer" />
      <min value="1" />
    </element>
  </differential>
</StructureDefinition>