<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="gd-Address" />
  <url value="http://ehelse.no/fhir/StructureDefinition/gd-Address" />
  <version value="0.1" />
  <name value="gd-Address" />
