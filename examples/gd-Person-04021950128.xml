<Person xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../STU3/fhir-all-xsd/fhir-single.xsd">
	<meta>
		<profile value="http://ehelse.no/fhir/StructureDefinition/gd-Person"/>
	</meta>
	<identifier><!-- Fødselnummeret til brukeren -->
		<extension url="http://ehelse.no/fhir/StructureDefinition/gd-person-identifier-status">
			<extension url="valueCode">
				<valueCode value="iBruk"/>
			</extension>
		</extension>
		<system value="urn:oid:2.16.578.1.12.4.1.4.1"/>
		<value value="The actual Fødselsnummer"/>
	</identifier>
	<identifier><!-- Foreign identifier -->
		<extension url="http://hl7.no/fhir/StructureDefinition/gd-country-code-3166-1-3">
			<extension url="valueCode">
				<valueCode value="NOR"/>
			</extension>
		</extension>
		<system value="http://ehelse.no/fhir/ValueSet/gd-person-identificationnumbertype-v05"/>
		<value value="utlendingsmyndighetenesIdentifikasjonsnummer"/>
	</identifier>
	<name> <!-- Navnet til brukeren -->
		<extension url="http://ehelse.no/fhir/StructureDefinition/gd-fregmetadata">
			<extension url="currentInfo">
				<valueBoolean value="true"/>
			</extension>			
		</extension>
		<family value="Søreng"/>
		<given value="Jannice"/>
	</name>
	<!-- Administrative Gender - the gender that the patient is considered to have for administration and record keeping purposes. -->
	<gender value="female"/>
</Person>
